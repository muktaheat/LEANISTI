<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.muktaheat.leanistix">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HomeActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".chest_main" />
        <activity android:name=".shoulder_main" />
        <activity android:name=".arms_main" />
        <activity android:name=".back_main" />
        <activity android:name=".legs_main" />
        <activity
            android:name=".chest_dayz"
            android:label="@string/title_activity_chest_dayz"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>